{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.png",
      "use": "@vercel/static"
    },
    {
      "src": "*.jpg",
      "use": "@vercel/static"
    },
    {
      "src": "*.jpeg",
      "use": "@vercel/static"
    },
    {
      "src": "*.gif",
      "use": "@vercel/static"
    },
    {
      "src": "*.webp",
      "use": "@vercel/static"
    },
    {
      "src": "*.ttf",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/products",
      "dest": "/products.html"
    },
    {
      "src": "/contact",
      "dest": "/contact.html"
    },
    {
      "src": "/signin",
      "dest": "/signin.html"
    },
    {
      "src": "/signup",
      "dest": "/signup.html"
    },
    {
      "src": "/login",
      "dest": "/login.html"
    },
    {
      "src": "/form",
      "dest": "/form.html"
    },
    {
      "src": "/chart",
      "dest": "/chart.html"
    },
    {
      "src": "/element",
      "dest": "/element.html"
    },
    {
      "src": "/button",
      "dest": "/button.html"
    },
    {
      "src": "/typography",
      "dest": "/typography.html"
    },
    {
      "src": "/widget",
      "dest": "/widget.html"
    },
    {
      "src": "/table",
      "dest": "/table.html"
    },
    {
      "src": "/blank",
      "dest": "/blank.html"
    },
    {
      "src": "/catecogry",
      "dest": "/catecogry.html"
    },
    {
      "src": "/productcard",
      "dest": "/productcard.html"
    },
    {
      "src": "/404",
      "dest": "/404.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}